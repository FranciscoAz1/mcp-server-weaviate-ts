{
  "name": "mcp-server-weaviate-ts",
  "version": "0.1.0",
  "description": "Weaviate MCP Server implemented in TypeScript",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts,js,json --exec \"tsx src/main.ts\"",
    "start": "node dist/main.js",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "node test/test-server.js",
    "bench": "node scripts/bench-mcp.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "weaviate",
    "typescript"
  ],
  "author": "Francisco Azeredo",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "weaviate-ts-client": "^2.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
