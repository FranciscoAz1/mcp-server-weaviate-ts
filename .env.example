# Environment variables for MCP Server Weaviate TS

# Weaviate connection settings
WEAVIATE_HOST=host.docker.internal:8080
WEAVIATE_SCHEME=http

# Server configuration
MCP_TRANSPORT="http"
MCP_HTTP_PORT=3000
MCP_HTTP_HOST=127.0.0.1

# Logging configuration
MCP_LOG_LEVEL=info
MCP_LOG_OUTPUT=stderr

# Security settings
MCP_READ_ONLY=false
MCP_DISABLED_TOOLS=

# Collections
MCP_DEFAULT_COLLECTION=DefaultCollection